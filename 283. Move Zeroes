class Solution {
    public void moveZeroes(int[] nums) {
       
        int indexofreachedzero =0;
        for(int i=0; i<nums.length;i++){
            int container= 0;
            for(int j=0; j<nums.length-i;j++){
                if(nums[j]==0 && j<nums.length-indexofreachedzero-1)
                {
                    container  = nums[j+1];
                    nums[j+1] = nums[j];
                    nums[j]= container;
                    
                }
            }
            indexofreachedzero++;
        }
        
             }
        
        }
